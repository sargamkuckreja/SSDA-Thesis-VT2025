      "source": [
        "# ========== Load Uploaded Files ==========\n",
        "with open(stage1_file, 'r', encoding='utf-8') as f:\n",
        "    stage1_chunks = f.read().splitlines()\n",
        "stage1_chunks = [chunk.strip() for chunk in stage1_chunks if chunk.strip()]\n",
        "\n",
        "with open(stage2_file, 'r', encoding='utf-8') as f:\n",
        "    stage2_chunks = f.read().splitlines()\n",
        "stage2_chunks = [chunk.strip() for chunk in stage2_chunks if chunk.strip()]\n",
        "\n",
        "with open(stage3_file, 'r', encoding='utf-8') as f:\n",
        "    stage3_chunks = f.read().splitlines()\n",
        "stage3_chunks = [chunk.strip() for chunk in stage3_chunks if chunk.strip()]"
      ],
      "metadata": {
        "id": "lL2izd4tbfqv"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "source": [
        "# ========== Run BERTopic for Each Stage ==========\n",
        "from umap import UMAP\n",
        "\n",
        "def run_bertopic(stage_chunks, stage_label):\n",
        "    print(f\"Running BERTopic for {stage_label}...\")\n",
        "    # Use UMAP for dimensionality reduction with n_components set to 5\n",
        "    umap_model = UMAP(n_components=5, random_state=42)\n",
        "    topic_model = BERTopic(language=\"english\", verbose=True, embedding_model=umap_model)\n",
        "    topics, probs = topic_model.fit_transform(stage_chunks)\n",
        "\n",
        "    topic_info = topic_model.get_topic_info()\n",
        "    output_filename = f\"bertopic_output_{stage_label}.csv\"\n",
        "    topic_info.to_csv(output_filename, index=False)\n",
        "    print(f\"Saved file: {output_filename}\")\n",
        "    return output_filename\n",
        "\n",
        "output1 = run_bertopic(stage1_chunks, \"Stage1\")\n",
        "output2 = run_bertopic(stage2_chunks, \"Stage2\")\n",
        "output3 = run_bertopic(stage3_chunks, \"Stage3\")"
      ],
      "cell_type": "code",
